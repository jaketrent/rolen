<li class="entry {{#if isFirst}}first{{/if}} {{ cat }}" data-id="{{ _id }}">
  <time datetime="{{ startDate }}">
    {{displayDates startDate endDate}}
  </time>
  <section>
    <header>
      <h2>{{ title }}</h2>
      <a href="/_utils/document.html?rolen/{{ _id }}" target="_blank" class="edit">&rarr;</a>
    </header>
    <p>
      {{markdown desc}}
    </p>
  </section>
</li>